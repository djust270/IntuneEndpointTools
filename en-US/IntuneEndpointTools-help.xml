    <command:details>
      <command:name>Disable-IntuneESP</command:name>
      <command:verb>Disable</command:verb>
      <command:noun>IntuneESP</command:noun>
      <maml:description>
        <maml:para>Disables the enrollment status page</maml:para>
      </maml:description>
    </command:details>
